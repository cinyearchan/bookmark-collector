{
  "name": "bookmark-collector",
  "version": "0.0.2",
  "description": "bookmark collector",
  "manifest_version": 3,
  "icons": {
    "16": "./icon.png",
    "32": "./icon.png"
  },
  "background": {
    "service_worker": "./background.js"
  },
  "permissions": [
    "activeTab",
    "tabs",
    "bookmarks"
  ],
  "action": {
    "default_title": "bookmark-collector",
    "default_icon": "icon.png"
  }
}